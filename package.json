  "private": true,
  "dependencies": {
    "prop-types": "latest",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "yarn concurrently \"yarn run start:server\" \"yarn run start:client\"",
